PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

insert {

    ?concept skos:notation ?notation

} where {

    ?concept skos:inScheme <https://data.ec2u.eu/concepts/euroscivoc> ;
        <http://publications.europa.eu/ontology/euvoc#xlNotation>/rdf:value ?notation

}