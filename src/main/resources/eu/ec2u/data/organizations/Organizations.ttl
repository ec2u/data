@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix ec2u: </terms/> .


org:Organization rdfs:subClassOf foaf:Agent .

org:FormalOrganization rdfs:subClassOf org:Organization .

org:OrganizationalCollaboration rdfs:subClassOf org:Organization .

org:OrganizationalUnit rdfs:subClassOf org:Organization .


org:subOrganizationOf rdfs:owl:inverseOf org:hasSubOrganization .

org:unitOf rdfs:owl:inverseOf org:hasUnit .

org:memberOf rdfs:owl:inverseOf org:hasMember .

org:headOf rdfs:subPropertyOf org:hasMember .


