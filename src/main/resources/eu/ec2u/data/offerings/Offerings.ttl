@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ec2u:	</terms/> .
@prefix void:	<http://rdfs.org/ns/void#> .
@prefix dct:	<http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .

</programs/> a ec2u:Dataset ;
	dct:title "EC2U Degree and Occupational Programs"@en ;
	dct:alternative "EC2U Programs"@en ;
	dct:description "Degree and occupational programs offered by EC2U allied universities."@en ;
	dct:license <http://creativecommons.org/licenses/by-nc-nd/4.0/> ;
    dct:rights "Copyright © 2022-2024 EC2U Alliance" ;
	dct:issued "2022-03-10"^^xsd:date ;
	void:rootResource ec2u:Program ;
	rdfs:isDefinedBy </datasets/offerings> .

</courses/> a ec2u:Dataset ;
	dct:title "EC2U Academic and Occupational Courses"@en ;
	dct:alternative "EC2U Courses"@en ;
	dct:description
		"Degree and standalone courses and other educational opportunities offered by EC2U allied universities."@en ;
	dct:license <http://creativecommons.org/licenses/by-nc-nd/4.0/> ;
    dct:rights "Copyright © 2022-2024 EC2U Alliance" ;
	dct:issued "2022-03-10"^^xsd:date ;
	void:rootResource ec2u:Course ;
	rdfs:isDefinedBy </datasets/offerings> .

</concepts/offering-topics> a skos:ConceptScheme ;
	dct:title "EC2U Educational Topics"@en .


########################################################################################################################

ec2u:Offering a owl:Class ;
	rdfs:subClassOf ec2u:Resource .

ec2u:Program a owl:Class ;
	rdfs:subClassOf ec2u:Offering, schema:EducationalOccupationalProgram .

ec2u:Course a owl:Class ;
	rdfs:subClassOf ec2u:Offering, schema:Course .

schema:EducationalOccupationalProgram a owl:Class ;
	rdfs:subClassOf schema:Thing .
	
schema:Course a owl:Class ;
	rdfs:subClassOf schema:LearningResource .

schema:CourseInstance a owl:Class ;
	rdfs:subClassOf schema:Event .

schema:LearningResource a owl:Class ;
	rdfs:subClassOf schema:CreativeWork .

schema:CreativeWork a owl:Class ;
	rdfs:subClassOf schema:Thing .


schema:courseCode a owl:DatatypeProperty ;
	rdfs:subPropertyOf schema:identifier .





