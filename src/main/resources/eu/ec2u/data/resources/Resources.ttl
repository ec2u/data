[] js:register '''

    function clip(text, length) {
        return text.replace(/^\\s*(.*\\.)\\s*\\n.*$/, function($0, $1) {
			return !$1 ? $0 : $1.length <= length ? $1: $1.substring(0, length-4)+" […]"
        })
    }

''' .