@prefix : <https://data.ec2u.eu/terms/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix ec2u: <https://data.ec2u.eu/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vivo: <http://vivoweb.org/ontology/core#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix schema: <https://schema.org/> .
@base <https://data.ec2u.eu/terms/> .



#################################################################
#    Object Properties
#################################################################



###  http://www.w3.org/ns/org#classification
org:classification rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf dct:type .


#################################################################
#    Classes
#################################################################


###  http://www.w3.org/ns/org#FormalOrganization
org:FormalOrganization rdf:type owl:Class .


###  http://www.w3.org/ns/org#OrganizationalUnit
org:OrganizationalUnit rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class .


###  https://data.ec2u.eu/terms/Actor
ec2u:Actor rdf:type owl:Class ;
           rdfs:subClassOf ec2u:Resource ;
           dct:description "Knowledge ecosystem actor."@en ;
           void:uriSpace "https://data.ec2u.eu/actors/" .


###  https://data.ec2u.eu/terms/Course
ec2u:Course rdf:type owl:Class ;
            rdfs:subClassOf ec2u:Resource ,
                            schema:Course ;
            void:uriSpace "https://data.ec2u.eu/courses/" .



###  https://data.ec2u.eu/terms/Event
ec2u:Event rdf:type owl:Class ;
           rdfs:subClassOf ec2u:Resource ,
                           schema:Event ;
           void:uriSpace "https://data.ec2u.eu/events/" .


###  https://data.ec2u.eu/terms/Person
ec2u:Person rdf:type owl:Class ;
            rdfs:subClassOf foaf:Person .


###  https://data.ec2u.eu/terms/Unit
ec2u:Unit rdf:type owl:Class ;
          rdfs:subClassOf org:OrganizationalUnit ,
                          ec2u:Resource ;
          void:uriSpace "https://data.ec2u.eu/units/" .


###  https://schema.org/Course
schema:Course rdf:type owl:Class ;
              rdfs:subClassOf schema:Thing .


###  https://schema.org/Event
schema:Event rdf:type owl:Class ;
             rdfs:subClassOf schema:Thing .
